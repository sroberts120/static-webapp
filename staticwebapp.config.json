{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html"
  },
  "responseOverrides": {
    "400": {
      "rewrite": "/index.html"
    },
    "401": {
      "redirect": "/index.html",
      "statusCode": 302
    },
    "403": {
      "rewrite": "/index.html"
    },
    "404": {
      "rewrite": "/index.html"
    }
  }
}
